{
    "name": "node_tradeup",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc --build && cp -R src/api/docs dist/api",
        "start": "node ./dist/index.js",
        "dev": " nodemon ./src/index.ts",
        "migration:dev": "npx sequelize db:migrate --env dev",
        "migration:prod": "npx sequelize db:migrate --env prod",
        "migration:dev:undo": "npx sequelize db:migrate:undo --env dev",
        "migration:prod:undo": "npx sequelize db:migrate:undo --env prod",
        "create:migration": "npx sequelize migration:generate",
        "migration:seed": "npx sequelize-cli db:seed:all",
        "swagger:generate": "node src/api/docs/swagger.js",
        "format": "prettier --write ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@prisma/client": "^5.20.0",
        "@types/joi": "^17.2.2",
        "@types/multer": "^1.4.12",
        "@types/sequelize": "^4.28.20",
        "aws-sdk": "^2.1691.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "joi": "^17.13.3",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "mysql2": "^3.11.3",
        "pg": "^8.13.0",
        "prettier": "^3.3.3",
        "sequelize": "^6.37.3",
        "sequelize-typescript": "^2.1.6",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^22.7.5",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "nodemon": "^3.1.7",
        "prisma": "^5.20.0",
        "sequelize-auto-migrations": "^1.0.3",
        "sequelize-cli": "^6.6.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    }
}
